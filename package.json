{
    "name": "Themis",
    "version": "1.0.0",
    "description": "Visualized AsyncStorage replacement for React Native during development",
    "main": "lib/index.js",
    "author": "Nima Shoghi <nima@nima.sh>",
    "license": "MIT",
    "scripts": {
        "build-server": "babel src/server.js --out-dir lib --source-maps",
        "run-server": "node lib/server.js",
        "start-server": "npm-run-all --serial build run-server",
        "start-viewer": "react-scripts start",
        "start": "npm-run-all --parallel start-server start-viewer"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.55",
        "@babel/core": "^7.0.0-beta.55",
        "@babel/preset-env": "^7.0.0-beta.55",
        "eslint": "^5.2.0",
        "prettier": "^1.14.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0-beta.55",
        "hapi": "^17.5.3",
        "jsonfile": "^4.0.0",
        "npm-run-all": "^4.1.3",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-json-view": "^1.19.1",
        "react-scripts": "1.1.4"
    }
}
